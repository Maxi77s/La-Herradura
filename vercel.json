{
    "version": 2,
    "builds": [
      {
        "src": "back/src/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "front/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "back/src/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "front/$1"
      }
    ],
    "env": {
      "DATABASE_URL": "@database_url",
      "PORT": "@port",
      "JWT_SECRET": "@jwt_secret"
    }
  }
  